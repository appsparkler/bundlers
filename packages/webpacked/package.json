{
  "name": "@me/webpacked",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prod": "rimraf dist/prod && cross-env NODE_ENV=production webpack --config='.rc-webpack' ",
    "dev": "rimraf dist/dev && cross-env NODE_ENV=development webpack --config='.rc-webpack' ",
    "dev-server": "serve dist/dev -l 8080",
    "all": "yarn clean && yarn prod && yarn dev",
    "all-serve": "yarn all && npm run dev-server"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "serve": "^11.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  }
}
